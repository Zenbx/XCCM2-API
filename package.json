{
  "name": "my-prisma-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "gen:certs": "node scripts/generate-certs.js",
    "dev:https": "npm run gen:certs && HTTPS=true SSL_CRT_FILE=certs/localhost.crt SSL_KEY_FILE=certs/localhost.key next dev -p 443",
    "synapse": "tsx src/synapse/server.ts",
    "synapse:check": "node scripts/check-synapse-env.js",
    "synapse:dev": "npm run synapse:check && npm run synapse",
    "test-synapse": "node synapse-standalone.cjs"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.28.6",
    "@hocuspocus/server": "^3.4.3",
    "@hocuspocus/transformer": "^3.4.3",
    "@huggingface/inference": "^4.13.9",
    "@prisma/client": "^6.19.1",
    "@supabase/supabase-js": "^2.90.1",
    "@tiptap/core": "^3.15.3",
    "@tiptap/pm": "^3.15.3",
    "@types/nodemailer": "^7.0.5",
    "@upstash/redis": "^1.36.1",
    "ably": "^2.17.0",
    "bcryptjs": "^3.0.3",
    "cookies-next": "^6.1.1",
    "docx": "^9.5.1",
    "dotenv": "^17.2.3",
    "https": "^1.0.0",
    "ioredis": "^5.9.2",
    "jose": "^6.1.3",
